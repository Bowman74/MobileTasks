<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileTasks.XForms.Views.Login">
	<StackLayout Orientation="Vertical">
		<Button Text="Facebook" Command="{Binding FacebookCommand}" />
		<Button Text="Microsoft" Command="{Binding MicrosoftCommand}" />
		<Button Text="Twitter" Command="{Binding TwitterCommand}" />
		<Button Text="Google" Command="{Binding GoogleCommand}" />
	</StackLayout>
</ContentPage>